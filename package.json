{
  "name": "react-redux-spa",
  "version": "1.0.0",
  "description": "",
  "author": "clux-hub <clux_hub@163.com>",
  "license": "MIT",
  "private": "true",
  "engines": {
    "node": ">=11.0.0"
  },
  "workspaces": [
    "public"
  ],
  "browserslist": [
    "ie >= 10"
  ],
  "scripts": {
    "patchActions": "clux-patch-actions patch",
    "type": "tsc --noEmit -p ./src && tsc --noEmit -p ./mock",
    "eslint": "eslint --cache ./",
    "stylelint": "stylelint \"./src/**/*.less\"",
    "checkDev": "yarn type & eslint --cache ./mock",
    "checkPro": "tsc --noEmit -p ./src & eslint --cache ./mock && eslint --cache ./ & yarn stylelint",
    "buildPolyfill": "cross-env NODE_ENV=production node build/buildPolyfill.js",
    "buildMock": "rimraf ./dist/mock && tsc -p ./mock",
    "build": "yarn checkPro && yarn buildMock && cross-env NODE_ENV=production PROJ_ENV=local node build/build.js",
    "start": "yarn checkDev && cross-env NODE_ENV=development PROJ_ENV=local DEBUG=true ts-node -P mock/tsconfig.json -r tsconfig-paths/register build/start.js",
    "runLocal": "node ./dist/local/start.js",
    "debug": "node --inspect=19248 --debug-brk"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.(ts|tsx|js|jsx)": "eslint --cache"
  },
  "resolutions": {
    "@types/node": "~14.14.7",
    "@types/jest": "~26.0.15",
    "@types/react": "~16.9.23",
    "@types/react-dom": "~16.9.5",
    "@types/express": "~4.17.9"
  },
  "dependencies": {
    "@clux/react-web": "~0.0.3",
    "axios": "~0.21.0",
    "pp-scroll": "1.1.0",
    "react": "~17.0.1",
    "react-dom": "~17.0.1",
    "react-redux": "~7.2.4"
  },
  "devDependencies": {
    "@clux/dev-pkg": "~0.0.2",
    "@clux/dev-utils": "~0.0.1",
    "@clux/dev-webpack": "~0.0.1",
    "@commitlint/cli": "~12.1.1",
    "@commitlint/config-conventional": "~12.1.1",
    "@types/express": "~4.17.9",
    "@types/jest": "~26.0.15",
    "@types/react": "~16.9.23",
    "@types/react-dom": "~16.9.5",
    "@types/react-redux": "~7.1.14",
    "commitizen": "~4.2.3",
    "husky": "~4.3.8",
    "jest": "~26.6.3",
    "less": "~3.12.2",
    "less-loader": "~7.1.0",
    "lint-staged": "~10.5.4",
    "typescript": "~4.2.0"
  }
}
